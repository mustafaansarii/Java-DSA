package helloaworld;

public class data_Type {
	public static void main(String[] args) {
		//Primitive Data Type
        //byte - 1 [-128 to 127]
        //short - 2
        //int - 4 1,2,3,
        //long - 8
        //float- 4 3.14
        //double - 8
        //char - 2 a,b,c,d
        //boolean 1 true/false

        byte age = 30;
        int phone= 1234567890;
        long phone2= 12345678900L;
        float pi= 3.14F;
        char letter= 'a';
        boolean isadult= false;
		System.out.println(pi);
	}

}

